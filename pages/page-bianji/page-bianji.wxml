<!--pages/page-bianji/page-bianji.wxml-->
<view class="page">
  <view class="option" wx:for="{{navs}}" data-idx="{{index}}" >
    <view class="user-name">{{item.name}}</view>
    <image class="user-head" src="{{item.img}}"></image>
    <image class="user-more" src="{{item.icon}}"></image>
    <view class="user-text">{{item.text}}</view>
  </view>
</view>
