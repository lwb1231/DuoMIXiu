  font-size: 22rpx;
  font-family: PingFang SC;
  font-weight: 500;
  color: #666666;
}

.bg-w {
  background: snow;
}

.th {
  border: 1rpx solid #DEDEDE;
  border-left: none;
  width: 40%;
  justify-content: center;
